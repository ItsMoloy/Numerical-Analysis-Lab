
function Moloy = Task_101 ()

   A = input("Matrix A : ");
   B = input("Matrix B : ");
   max_A = max(max(A));
   fprintf("Max value of A : %d\n", max_A);
   if size(A,2)==size(B,1)
      Multiplctn = A * B;
      fprintf("Multiplication :\n");
      disp (Multiplctn);
   else
      fprintf ("Matrix multiplication is not possible!\n");
   end

endfunction
